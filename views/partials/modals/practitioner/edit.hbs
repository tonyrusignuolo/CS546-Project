<div class="modal fade" id="practitionerEdit" tabindex="-1" role="dialog">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h3 class="modal-title">Edit Practitioner</h3>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<i class="material-icons">clear</i>
				</button>
			</div>
			<div class="modal-body">
				<form action="/practitioner/edit" method="POST" enctype="multipart/form-data">
					<div class="form-row">
						<div class="form-group col-md-12">
							<label for="name">Practitioner Name</label>
							<input type="text" class="form-control" id="name" name="name">
						</div>
					</div>
					<div class="form-row">
						<div class="form-group col-md-6">
							<label for="lat">Latitude</label>
							<input type="number" step="0.001" class="form-control" id="lat" name="lat">
						</div>
						<div class="form-group col-md-6">
							<label for="long">Longitude</label>
							<input type="number" step="0.001" class="form-control" id="long" name="long">
						</div>
					</div>
					<b>Procedures</b>
					<div id="procedures">
						<div class="form-row">
							<div id="procedures1" class="form-group col-md-6">
							</div>
							<div id="procedures2" class="form-group col-md-6">
							</div>
						</div>
					</div>
					<div class="form-row">
						<div class="form-group col-md-12">
							<label for="insuranceProvider" class="bmd-label-floating">Insurance Provider</label>
							<select class="form-control" data-style="btn btn-link" id="insuranceProvider"
								name="insuranceProvider">
								<option value="">Select Insurance Provider</option>
								<option value="Blue Cross">Blue Cross</option>
								<option value="Cigna">Cigna</option>
								<option value="United">United</option>
								<option value="Delta">Delta</option>
							</select>
						</div>
					</div>
					<div class="modal-footer">
						<button type="submit" class="btn btn-link">Save</button>
						<button type="button" class="btn btn-danger btn-link" data-dismiss="modal">Close</button>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>